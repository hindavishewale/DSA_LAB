class student
{
public:
	string id;
	string name;
};
class hashtable
{
public:
	student s1[10];
	student s2[10];
	
void create(int r,string n)
{
}

void ascii(string id)
{
int c;
for(int i=0;i<id.size();i++)
{
    int cnt=int(id[i]);
    cout<<"acii of "<<id[i]<<cnt<<endl;
    c+=cnt;
}
    cout<<"total ascii value of string :"<<c;
}


};
int main()
{
