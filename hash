#include <iostream>
using namespace std;
class Data
{
    public:
        int roll;
        string name;
        
        Data()
        {
            roll=-1;
            name="";
        }
        
};
class Hash
{  
public:
    static const int size=10;
    Data d[size];
    int hashfunction(int key)
    {
       return key%size;  
    }
    
    void insert(string name, int roll)
    {
        int index=hashfunction(roll);
        int startindex=index;
        
        while(d[index].roll!=-1)
        {
            index=(index+1)%size;
            if(startindex==index)
            {
                cout<<"hash table full!!";
                return ;
            }
        }
        d[index].name=name;
        d[index].roll=roll;
    }
    void display()
    {
        for(int i=0;i<size;i++)
        {
            if(d[i].roll!=-1)
            {
                cout<<"index"<<i<<"\t"<<"roll number:"<<d[i].roll<<"\t"<<"name:"<<d[i].name<<endl;  
            }
            else
            {
                cout<<"index"<<i<<"\t"<<"empty"<<endl;
            }
    }
    }    
};    
int main() 
{
     Hash h;
     string name;
     int rn,n;
     cout<<"enter number of student: ";
     cin>>n;
     for(int i=0;i<n;i++)
     {
     cout<<"enter roll number: ";
     cin>>rn;
     cout<<"enter name: ";
     cin>>name;
     h.insert(name,rn);
     }
    
     h.display();
     

    return 0;
}
