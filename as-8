#include <iostream>
using namespace std;
int main()
{
   int n;
   cout<<"enter number of keys: ";
   cin>>n;
   int key[n];
   int p[10],q[10],w[10][10],c[10][10],r[10][10];
   cout<<"enter "<<n<<"keys: ";
   for(int i=0;i<n;i++)
   {
       cin>>key[i];
   }
   cout<<"enter"<<n<<"succ prob"<<endl;
   for(int i=1;i<=n;i++)
   {
       cin>>p[i];
   }
    cout<<"enter"<<n+1<<"succ prob"<<endl;
    for(int i=0;i<=n;i++)
   {
       cin>>q[i];
   }
 
for(int i=0;i<=n;i++)
{
    for(int j=i;j<=n;j++)
    {
                if(j==i)
                 {
                 w[i][j]=q[i];
                 cout<<i<<j<<"\t"<<w[i][j]<<endl;
                 }
                 else
                {
                 w[i][j]=w[i][j-1]+q[j]+p[j];
                 cout<<i<<j<<"\t"<<w[i][j]<<endl;
                }   
    }
}
    return 0;
}
